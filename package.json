{
  "name": "osrs-hiscores",
  "version": "1.1.3",
  "license": "MIT",
  "description": "A Promise based Old School RuneScape hiscores API",
  "homepage": "https://osrslogs.com",
  "bugs": {
    "url": "https://github.com/osrslogs/osrs-hiscores/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osrslogs/osrs-hiscores.git"
  },
  "author": {
    "name": "hsamoht",
    "email": "contact@osrslogs.com"
  },
  "keywords": [
    "osrslogs",
    "runescape",
    "osrs",
    "hiscores",
    "api"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --p tsconfig.build.json",
    "build:watch": "npm run build -- -w",
    "test": "jest --config jest.config.json",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint -c .eslintrc.json **/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "preversion": "npm run lint",
    "version": "npm run test && git add -A src",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/cheerio": "^0.22.16",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.2.0",
    "typescript": "^3.8.3"
  }
}
